{
  "name": "documentation-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "generate-files": "node ./scripts/build-documentation-site-files.js",
    "dev": "run-s generate-files next:dev",
    "build": "run-s generate-files next:build",
    "build:baseui": "cd .. && yarn build",
    "rebuild": "run-s build:baseui build",
    "serve": "static-server public --port 8081",
    "next:dev": "next dev",
    "next:build": "NODE_OPTIONS=--max_old_space_size=8192 next build",
    "thumbs:generate": "svgr --template ./components/thumbs/template.js --replace-attr-values \"#F6F6F6={props.colors[0]}\" --replace-attr-values \"#E2E2E2={props.colors[1]}\" --replace-attr-values \"#CBCBCB={props.colors[2]}\" -d ./components/thumbs/components ./components/thumbs/svg"
  },
  "dependencies": {
    "npm-run-all2": "^6.2.3",
    "@babel/parser": "^7.25.8",
    "@babel/traverse": "^7.25.7",
    "@octokit/rest": "^21.0.2",
    "just-omit": "^1.1.0",
    "just-pick": "^2.1.0",
    "core-js": "^2.5.7",
    "react-twitter-embed": "^2.0.8",
    "react-simple-code-editor": "^0.10.0",
    "react-view": "^2.3.3",
    "react-vis": "^1.12.1",
    "react-compare-image": "^2.0.3",
    "prism-react-renderer": "^1.0.1",
    "raw-loader": "^4.0.2",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-mdx": "^1.2.0",
    "@mdx-js/tag": "^0.20.3",
    "@next/mdx": "^13.3.1",
    "react-icons": "^3.8.0",
    "next": "^14.2.15",
    "file-loader": "^6.2.0",
    "js-tokens": "^9.0.0"
  },
  "resolutions": {
    "js-tokens": "9.0.0"
  },
  "devDependencies1": {
    "@date-io/date-fns": "^3.0.0",
    "@mona-health/react-input-mask": "3.0.3",
    "card-validator": "10.0.0",
    "csstype": "^3.1.3",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "memoize-one": "^6.0.0",
    "polished": "^4.3.1",
    "popper.js": "^1.16.1",
    "react-dropzone": "14.2.9",
    "react-focus-lock": "^2.13.2",
    "react-is": "^18.3.1",
    "react-movable": "^3.3.1",
    "react-multi-ref": "^1.0.2",
    "react-range": "^1.10.0",
    "react-uid": "^2.3.3",
    "react-virtualized": "^9.22.5",
    "react-virtualized-auto-sizer": "^1.0.24",
    "react-window": "^1.8.10",
    "resize-observer-polyfill": "^1.5.1",
    "styletron-react": "^6.1.1",
    "styletron-standard": "^3.1.0",
    "styletron-engine-atomic": "^1.0.0",
    "styletron-engine-monolithic": "^1.0.0",
  }
}
